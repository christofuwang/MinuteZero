# App server settings
APP_HOST=0.0.0.0
APP_PORT=8000
CORS_ORIGINS=http://localhost:3000,http://127.0.0.1:3000

# Archia API credentials
ARCHIA_API_KEY=replace_me

# Archia API endpoint settings
# Option A (recommended): set full URL from Archia docs/dashboard
ARCHIA_TRANSCRIPTION_URL=

# Option B: compose from base URL + path
ARCHIA_BASE_URL=
ARCHIA_TRANSCRIPTION_PATH=/v1/transcriptions
ARCHIA_TRANSCRIPTION_MODEL=whisper-1
